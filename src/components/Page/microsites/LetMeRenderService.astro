import OrderRenderNow from '@components/Page/microsites/microparts/OrderRenderNow.astro';
import RenderDiscounts from '@components/Page/microsites/microparts/RenderDiscounts.astro';
import TodayBlenderVersion from '@components/Elements/SparePart/TodayBlenderVersion.astro';

export interface Props{
    nsfw?:boolean;
}

let detachedGenital:boolean = true;
let noNeedNSFW:boolean = false;
let addHeaderTitle = 'CHEAP & HIGH QUALITY BLENDER 3D MODEL & SCENE RENDER SERVICE';
let addMetaTitle = 'CHEAP & HIGH QUALITY BLENDER 3D MODEL & SCENE RENDER SERVICE';
let addKeywords:boolean = 'Blender, Commission, Render, Renderfarm, Contact us';
let addDescriptions = 'Let us render your Blender file as high quality as possible, and sit back.';
let skipHeader:boolean = false;
let skipNav:boolean = false;
let skipFooter:boolean = false;
let disableOverwhelmStatement:boolean = true;
let author = 'JOELwindows7';

let maxNumCommissionSlot:number = 8;

const{
    nsfw = false,
} = Astro.props.frontmatter || Astro.props as Props;
---
<CoreMicrosite
