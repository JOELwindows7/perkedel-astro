export interface Props{
    nsfw?:boolean;
}

let detachedGenital:boolean = true;
let noNeedNSFW:boolean = true;
let addHeaderTitle = 'NOTICE: Retirement of Tumblr accounts & blogs';
let addMetaTitle = 'We have been kept blocked by so many in Tumblr';
let addKeywords:string = 'Tumblr, toxic, community, blocked, immature, Contact us';
let addDescriptions:string = 'We have abandoned our Tumblr accounts, but the account kept up for posterity.';
let skipHeader:boolean = false;
let skipNav:boolean = false;
let skipFooter:boolean = false;
let disableOverwhelmStatement:boolean = true;
let author = 'JOELwindows7';

let maxNumCommissionSlot:number = 8;
let maxNumPerTransactionSlot:number = 240;
let maxGigabytePerFile:number = 2;

const{
    nsfw = false,
} = Astro.props.frontmatter || Astro.props as Props;

const canonicalURL:URL = new URL(Astro.url.pathname, Astro.site);
const padd:string = `${canonicalURL}/apps/microsites/`;
---
